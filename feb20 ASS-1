a = int(input("Enter a range: "))  
b = int(input("Enter b range: "))  
  
for num in range(a,b + 1):  
   if num > 1:  
       for i in range(2,num):  
           if (num % i) == 0:  
               break  
       else:  
           print(num)
           
ouput:-
enter a range: 1
enter b range: 10
4
